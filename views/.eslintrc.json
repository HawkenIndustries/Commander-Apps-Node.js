{   
    "extends":[
        "../eslintDefaults.js"
    ],
    "env": {
        "browser": true,
        "jquery": true
    },
    "globals": {
        "_":true,
        "moment":true,
        "Materialize":true,
        "uuid":true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "valid-jsdoc":"warn",
        "semi":["error","always"]
    }
}